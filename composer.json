{
    "name": "silverstripers/form-submission-manager",
    "description": "Manage user forms submissions",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "forms", "module"],
    "license": "BSD-3-Clause",
    "authors": [
    {
        "name": "Nivanka Fonseka",
        "email": "nivanka@silverstripers.com"
    }],
    "require": {
        "silverstripe/framework": "4.* | 5.*",
        "dnadesign/silverstripe-elemental-userforms": "*",
        "silverstripers/udf-spam-protection": "*"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
